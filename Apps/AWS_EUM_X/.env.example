# AWS Configuration
AWS_REGION=us-east-1
AWS_AUTH_METHOD=auto
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=

# Application Settings
PORT=3000
HOST=0.0.0.0
LOG_LEVEL=info

# Rate Limiting
RATE_LIMIT_TPS=5
RATE_LIMIT_BURST=10

# AWS Features (Optional)
SMS_CONFIG_SET_ARN=
OPT_OUT_LIST=
SMS_MESSAGE_TYPE=TRANSACTIONAL

# Application Behavior
MAX_HISTORY=1000
ENABLE_WIZARD=true
SESSION_SECRET=

# Development Only
NODE_ENV=development
